{
  "name": "template-nextjs-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TAILWIND_MODE=watch NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint . --cache",
    "format": "prettier --write '**/*.{js,ts,tsx,json}'",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "generate-css-types": "tailwindcss-classnames -i ./tailwind.config.js -o ./src/utils/tailwindcss.ts"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "next lint . --cache --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "3.0.18",
    "tailwindcss-classnames": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.0",
    "@storybook/addon-actions": "6.4.18",
    "@storybook/addon-essentials": "6.4.18",
    "@storybook/addon-links": "6.4.18",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.18",
    "@storybook/manager-webpack5": "6.4.18",
    "@storybook/react": "6.4.18",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.39",
    "@types/tailwindcss": "3.0.7",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "autoprefixer": "10.4.2",
    "babel-jest": "27.5.0",
    "babel-loader": "8.2.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.0",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.3",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  }
}
